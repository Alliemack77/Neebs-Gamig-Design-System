nal":{"line":5,"column":4},"generated":{"line":384,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":6,"column":4},"generated":{"line":385,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":7,"column":4},"generated":{"line":386,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":8,"column":4},"generated":{"line":387,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":9,"column":4},"generated":{"line":388,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":10,"column":4},"generated":{"line":389,"column":2}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":10,"column":25},"generated":{"line":390,"column":0}},{"source":"scss\\utilities\\_sr-only.scss","name":null,"original":{"line":11,"column":4},"generated":{"line":391,"column":2}}],"sources":{"scss\\abstracts\\_colors.scss":"/* Colors */\r\n$clr-black: hsl(0 0% 0%);\r\n$clr-white: hsl(0 0% 100%);\r\n$clr-logo: hsl(42 99% 58%);\r\n\r\n/* Shages */\r\n$black-25: transparentize($clr-black, .25);\r\n$white-25: transparentize($clr-white, .25);\r\n$logo-25: transparentize($clr-logo, .25);\r\n\r\n$black-50: transparentize($clr-black, .50);\r\n$white-50: transparentize($clr-white, .50);\r\n$logo-50: transparentize($clr-logo, .50);\r\n\r\n$black-75: transparentize($clr-black, .75);\r\n$white-75: transparentize($clr-white, .75);\r\n$logo-75: transparentize($clr-logo, .75);\r\n\r\n/* Background color utilities */\r\n.bg-black { background-color: $clr-black ; }\r\n.bg-white { background-color: $clr-white ; }\r\n.bg-logo { background-color: $clr-logo ; }\r\n\r\n/* Background shades */ \r\n.bg-black-50 { background-color: transparentize($clr-black, .5); }\r\n.bg-white-50 { background-color: transparentize($clr-white, .5); }\r\n.bg-logo-50 { background-color: transparentize($clr-logo, .5); }\r\n\r\n/* Text color utilities */\r\n.text-black { color: $clr-black; }\r\n.text-white { color: $clr-white; }\r\n.text-logo { color: $clr-logo; }\r\n\r\n/* Text shades */ \r\n.text-black-25 { color: transparentize($clr-black, .25); }\r\n.text-white-25 { color: transparentize($clr-white, .25); }\r\n.text-logo-25 { color: transparentize($clr-logo, .25); }\r\n\r\n.text-black-50 { color: transparentize($clr-black, .5); }\r\n.text-white-50 { color: transparentize($clr-white, .5); }\r\n.text-logo-50 { color: transparentize($clr-logo, .5); }\r\n\r\n.text-black-75 { color: transparentize($clr-black, .75); }\r\n.text-white-75 { color: transparentize($clr-white, .75); }\r\n.text-logo-75 { color: transparentize($clr-logo, .75); }\r\n\r\n","scss\\abstracts\\_fonts.scss":"/* Font-sizes */\r\n$fs-900: 6rem; \r\n$fs-800: 3rem; \r\n$fs-700: 2.25rem;\r\n$fs-600: 2rem;\r\n$fs-500: 1.5rem; \r\n$fs-400: 1.125rem; // body - 18px\r\n$fs-300: 1rem; \r\n$fs-200: 0.875rem;\r\n\r\n/* Font-families */\r\n$ff-heading: 'Poppins', sans-serif;\r\n$ff-body: 'Quicksand', sans-serif;\r\n$ff-accent: 'Bangers', cursive;\r\n\r\n/* Font-weights */\r\n$fw-700: 700;\r\n$fw-500: 500;\r\n$fw-400: 400;\r\n$fw-300: 300;\r\n\r\n","scss\\abstracts\\_variables.scss":"/*  Flex and Grid */\r\n$gap: 2rem;\r\n\r\n/*  Flow */\r\n$flow-space-xs: 1rem;\r\n$flow-space-sm: 1.5rem; // 24px\r\n$flow-space-md: 2rem; // 32px\r\n$flow-space-lg: 2.25rem; // 36px\r\n$flow-space-xl: 3rem; // 48px\r\n$flow-space-xxl: 4rem; // 64px\r\n$flow-space-xxxl: 8rem; // 128px\r\n","scss\\base\\_reset.scss":"@use '../abstracts/fonts' as *;\r\n\r\n/* Box-sizing rules*/\r\n*, \r\n*::after, \r\n*::before {\r\n    box-sizing: border-box;\r\n}\r\n\r\n/* Remove default margins */\r\nbody,\r\nh1, \r\nh2, \r\nh3, \r\nh4, \r\nh5, \r\nh6, \r\np {\r\n    margin: 0;\r\n}\r\n\r\nh1, \r\nh2 {\r\n    font-weight: $fw-400;\r\n}\r\n\r\n/* Set core body defaults */\r\nbody {\r\n    line-height: 1.5;\r\n    min-height: 100vh;\r\n}\r\n\r\n/* Make images easier to work with */\r\nimg, \r\npicture {\r\n    max-width: 100%;\r\n    display: block;\r\n}\r\n\r\n/* Inherit fonts for form elements */\r\ninput, \r\nbutton, \r\ntextarea, \r\nselect {\r\n    font: inherit;\r\n}\r\n\r\n/* Remove padding and styling on all ul's with a class */\r\nul[class] {\r\n    list-style: none;\r\n    padding: 0;\r\n}\r\n\r\n/* A elements that don't have a class get defautl styles */\r\na:not([class]){\r\n    text-decoration-skip-ink: auto;\r\n}\r\n\r\n/* remove animations for people who've turned them off */\r\n@media (prefers-reduced-motion: reduce) {\r\n    *,\r\n    *::before,\r\n    *::after {\r\n      animation-duration: 0.01ms !important;\r\n      animation-iteration-count: 1 !important;\r\n      transition-duration: 0.01ms !important;\r\n      scroll-behavior: auto !important;\r\n    }\r\n}","scss\\base\\_typography.scss":"@use 'abstracts/fonts' as *;\r\n@use 'abstracts/colors' as *;\r\n\r\n/* Font-family */\r\n.ff-heading { font-family: $ff-heading };\r\n.ff-body { font-family: $ff-body };\r\n.ff-accent { font-family: $ff-accent };\r\n\r\n/* Font-size */\r\n.fs-900 { font-size: $fs-900 };\r\n.fs-800 { font-size: $fs-800 };\r\n.fs-700 { font-size: $fs-700 };\r\n.fs-600 { font-size: $fs-600 };\r\n.fs-500 { font-size: $fs-500 };\r\n.fs-400 { font-size: $fs-400 };\r\n.fs-300 { font-size: $fs-300 };\r\n.fs-200 { font-size: $fs-200 };\r\n\r\n/* Font-weight */\r\n.fw-700 { font-weight: $fw-700 };\r\n.fw-500 { font-weight: $fw-500 };\r\n.fw-400 { font-weight: $fw-400 };\r\n.fw-300 { font-weight: $fw-300 };\r\n\r\n/* Line-height */\r\n.fs-900, \r\n.fs-800, \r\n.fs-700 {\r\n    line-height: 1.1;\r\n}\r\n\r\n/* Character spacing */\r\n.letter-spacing-1 { letter-spacing: 5px; }\r\n\r\n/* Random styles */\r\n.uppercase { text-transform: uppercase;}\r\n\r\n.numbered-title { \r\n    font-size: $fs-500;\r\n    margin-bottom: 1rem;\r\n    \r\n    span {\r\n        font-weight: $fw-700;\r\n        color: $white-75;\r\n        margin-right: .75em;\r\n    }\r\n    \r\n}\r\n\r\n\r\n\r\n\r\n\r\n","scss\\base\\_base.scss":"@use '../abstracts/colors' as *;\r\n@use '../abstracts/fonts' as *;\r\n\r\nbody {\r\n    font-family: $ff-heading;\r\n    font-size: $fs-400;\r\n    font-weight: $fw-300;\r\n    color: $clr-white;\r\n    background-color: $clr-black;\r\n}\r\n\r\n","scss\\utilities\\_container.scss":".container {\r\n    padding-inline: 2em; \r\n    margin-inline: auto;\r\n    max-width: 87.5rem; //1400px\r\n}\r\n\r\n","scss\\utilities\\_flex.scss":"@use '../abstracts/variables' as *;\r\n\r\n.flex {\r\n    display: flex;\r\n    gap: $gap;\r\n}\r\n\r\n// equal columns\r\n.column {\r\n    flex: 1 1 100%;\r\n}\r\n\r\n\r\n\r\n\r\n","scss\\utilities\\_flow.scss":"@use '../abstracts/variables' as *;\r\n\r\n// selects any child element that has a directly adjacet sibiling (a sibling directly before it)\r\n.flow-xs > * + * {\r\n    margin-top: $flow-space-xs;\r\n}\r\n\r\n.flow-sm > * + * {\r\n    margin-top: $flow-space-sm;\r\n}\r\n\r\n.flow-md > * + * {\r\n    margin-top: $flow-space-md;\r\n}\r\n\r\n.flow-lg > * + * {\r\n    margin-top: $flow-space-lg;\r\n}\r\n\r\n.flow-xl > * + * {\r\n    margin-top: $flow-space-xl;\r\n}\r\n\r\n.flow-xxl > * + * {\r\n    margin-top: $flow-space-xxl;\r\n}\r\n\r\n.flow-xxxl > * + * {\r\n    margin-top: $flow-space-xxxl;\r\n}\r\n\r\n","scss\\utilities\\_grid.scss":"@use '../abstracts/variables' as *;\r\n\r\n.grid {\r\n    display: grid;\r\n    gap: $gap;\r\n}","scss\\utilities\\_sr-only.scss":"// visually hides content but leaves it in the DOM\r\n.sr-only {\r\n    position: absolute;\r\n    width: 1px;\r\n    height: 1px;\r\n    padding: 0;\r\n    margin: -1px;\r\n    overflow: hidden;\r\n    clip: rect(0, 0, 0, 0);\r\n    white-space: nowrap; /* added line */\r\n    border: 0;\r\n}"},"lineCount":null}},"error":null,"hash":"7a057b6f06581db4ebd4d27248d26152","cacheData":{"env":{}}}